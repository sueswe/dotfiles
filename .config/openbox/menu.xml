<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="SoundMixer">
			<action name="Execute">
				<execute>pavucontrol</execute>
			</action>
		</item>
		<item label="Terminal emulator">
			<action name="Execute">
				<execute>x-terminal-emulator</execute>
			</action>
		</item>
        
		<item label="Screenshot">
			<action name="Execute">
				<execute>shutter</execute>
			</action>
		</item>
        
		<item label="Hex jeex">
			<action name="Execute">
				<execute>jeex</execute>
			</action>
		</item>
        
		<item label="guvcview">
			<action name="Execute">
			<execute>guvcview</execute>
			</action>
		</item>
        
    <item label="Thunar">
			<action name="Execute">
			<execute>thunar</execute>
			</action>
		</item>

		<item label="Google">
			<action name="Execute">
			<execute>google-chrome</execute>
			</action>
		</item>

		<!-- This requires the presence of the 'menu' package to work -->
		<menu id="/Debian"/>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
